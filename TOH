#include <stdio.h>
#include <stdlib.h>

void TOH(int n, char src, char helper, char des) {
    if (n == 1) {
        printf("Move disk %d from %c to %c\n", n, src, des);
        return;
    }
    TOH(n - 1, src, des, helper);
    printf("Move disk %d from %c to %c\n", n, src, des);
    TOH(n - 1, helper, src, des);
}

int main() {
    int n;
    printf("Enter the number of disks: ");
    scanf("%d", &n);
    printf("\nSequence of moves:\n");
    TOH(n, 'S', 'H', 'D'); 
    return 0;
}
